10 REM THE COMPUTER WILL GENERATE A RAND
11 REM OM NUMBER
20 R = INT(RND(1)*100)+1
25 GUESSES = 0
30 INPUT "GUESS THE NUMBER:";A
35 GUESSES = GUESSES + 1
40 IF A = R THEN GOTO 100
50 IF A < R THEN PRINT "TOO LOW"
60 IF A > R THEN PRINT "TOO HIGH"
70 PRINT " TRY AGAIN"
80 GOTO 30
100 PRINT "YOU GOT IT!"
105 PRINT "IT TOOK YOU";GUESSES;"GUESSES"
110 END
